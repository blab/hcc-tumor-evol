				<multivariateTraitLikelihood idref="Y_coord.traitLikelihood"/>
			</trait>
			<trait name="X_coord" tag="X_coord">
				<multivariateTraitLikelihood idref="X_coord.traitLikelihood"/>
			</trait>

			<!-- END Ancestral state reconstruction                                      -->


			<!-- START Multivariate diffusion model                                      -->
			<multivariateDiffusionModel idref="Y_coord.diffusionModel"/>
			<multivariateTraitLikelihood idref="Y_coord.traitLikelihood"/>
			<multivariateDiffusionModel idref="X_coord.diffusionModel"/>
			<multivariateTraitLikelihood idref="X_coord.traitLikelihood"/>

			<!-- END Multivariate diffusion model                                        -->

		</logTree>
	</mcmc>
	
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
	
</beast>
